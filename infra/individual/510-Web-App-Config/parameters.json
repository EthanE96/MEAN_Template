{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "webAppName": {
      "value": "mean-app-test"
    },
    "appSettings": {
      "value": {
        "API_URL": "your-api-url",
        "PORT": "3000",
        "ENV": "production",
        "AZURE_CLIENT_ID": "your-azure-client-id",
        "AZURE_RESOURCE_GROUP": "MEAN_Template",
        "AZURE_SUBSCRIPTION_ID": "your-subscription-id",
        "COSMOS_ACCOUNT_NAME": "your-cosmos-account",
        "COSMOS_DATABASE": "your-database-name",
        "GITHUB_CLIENT_ID": "your-github-client-id",
        "GITHUB_CLIENT_SECRET": "your-github-client-secret",
        "GITHUB_CALLBACK_ENDPOINT": "/api/auth/callback/github",
        "GOOGLE_CLIENT_ID": "your-google-client-id",
        "GOOGLE_CLIENT_SECRET": "your-google-client-secret",
        "GOOGLE_CALLBACK_ENDPOINT": "/api/auth/callback/google",
        "GROQ_API_KEY": "your-groq-api-key",
        "SESSION_SECRET": "your-session-secret-key",
        "SESSION_COOKIE_NAME": "session",
        "SESSION_TIMEOUT_MINUTES": "60",
        "SECURE_SESSION_COOKIE": "false",
        "UI_URL": "your-ui-url",
        "UI_SUCCESS_URL": "your-success-url",
        "UI_FAILURE_URL": "your-failure-url",
        "SEED": "false",
        "DOCKER_ENABLE_CI": "true",
        "WEBSITE_USE_PLACEHOLDER": "0",
        "WEBSITES_ENABLE_APP_SERVICE_STORAGE": "false"
      }
    }
  }
}